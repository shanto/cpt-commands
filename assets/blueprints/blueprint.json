{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "landingPage": "/wp-admin/admin.php?page=cpt-commands-settings",
  "steps": [
    {
      "step": "defineWpConfigConsts",
      "consts": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": true,
        "SCRIPT_DEBUG": true
      }
    },
    {
      "step": "installPlugin",
      "pluginData": {
        "resource": "wordpress.org/plugins",
        "slug": "plugin-check"
      }
    },
    {
      "step": "unzip",
      "zipFile": {
        "resource": "url",
        "url": "https://raw.githubusercontent.com/shanto/cpt-commands/master/assets/blueprints/types.zip"
      },
      "extractToPath": "/wordpress/wp-content/mu-plugins"
    },
    {
      "step": "importWxr",
      "file": {
        "resource": "url",
        "url": "https://raw.githubusercontent.com/shanto/cpt-commands/master/assets/blueprints/stories.wxr"
      }
    },
    {
      "step": "login"
    }
  ]
}